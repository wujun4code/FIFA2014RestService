@model IEnumerable<RestServiceWeb.Models.Db.Match>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.SerialNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FIFASerialNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartUTCTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndUTCTime)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Running)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsLive)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.SerialNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FIFASerialNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartUTCTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndUTCTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Running)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsLive)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
